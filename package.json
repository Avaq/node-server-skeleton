{
  "name": "PROJECT_NAME",
  "version": "0.1.0",
  "description": "PROJECT_DESCRIPTION",
  "main": "src/index.js",
  "repository": "PROJECT_REPOSITORY",
  "scripts": {
    "check-security": "nsp check",
    "clean": "rimraf npm-debug.log coverage",
    "dev": "DEBUG=*,-express:* node-dev --stack_trace_limit=100 .",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "pre-commit": "npm run lint && npm run test:unit && npm run test:integration && npm run check-security",
    "sanctuary-env": "node scripts/sanctuary-env",
    "setup": "npm run sanctuary-env && cp scripts/hooks/* .git/hooks && git config push.followTags true",
    "start": "node .",
    "test": "npm run clean && npm run lint && npm run test:coverage && npm run test:integration",
    "test:integration": "NODE_ENV=test mocha test/integration/**/*.test.js",
    "test:unit": "NODE_ENV=test mocha test/unit/**/*.test.js",
    "test:coverage": "NODE_ENV=test node node_modules/.bin/isparta cover --root src --report text --report html _mocha -- --reporter dot test/unit/**/*.test.js"
  },
  "engines": {
    "node": "^4.0.0"
  },
  "author": "PROJECT_AUTHOR",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "config": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "fluture": "^4.0.3",
    "http-errors": "^1.4.0",
    "json5": "^0.5.0",
    "jwt-simple": "^0.5.0",
    "micromatch": "^2.3.11",
    "momi": "^0.2.0",
    "ramda": "^0.22.1",
    "request": "^2.72.0",
    "sanctuary": "^0.11.1",
    "semver": "^5.3.0",
    "tcomb": "^3.0.0",
    "tcomb-validation": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-things": "^0.2.0",
    "eslint": "^3.5.0",
    "eslint-config-warp": "^1.1.0",
    "glob": "^7.0.0",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "node-dev": "^3.0.0",
    "nsp": "^2.3.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1"
  }
}
