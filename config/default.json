    "gid": "node"

  },

  //Web server configuration.
  "server": {

    "http": {
      "enabled": true,
      "host": "localhost",
      "port": 80
    },

    "https": {
      "enabled": false,
      "host": "localhost",
      "port": 443,
      "key": null,
      "cert": null
    }

  }

}
